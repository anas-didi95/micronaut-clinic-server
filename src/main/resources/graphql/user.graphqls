type User {
  id: String!
  fullName: String!
  isDeleted: Boolean!
  createdDate(format: String): String!
  createdBy: String!
  updatedDate(format: String): String
  updatedBy: String
  version: Int!
}

type UserSearch {
  resultList: [User]!
  pagination: Pagination!
}

extend type Query {
  userSearch(page: Int! = 1, size: Int! = 10): UserSearch!
  user(id: String!): User!
}
